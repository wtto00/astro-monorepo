{
  "name": "vite-multiple-packages",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "dev": "node scripts/app.mjs",
    "build": "node scripts/app.mjs build",
    "new:project": "node scripts/newProject.mjs",
    "prepare": "husky install && husky set .husky/pre-commit \"npx lint-staged\"",
    "format": "prettier --write .",
    "lint": "eslint --fix --color src/**/*.{js,ts,jsx,vue}"
  },
  "dependencies": {
    "pinia": "^2.0.12",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.5.0",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "inquirer": "^8.2.2",
    "lint-staged": "^12.3.7",
    "prettier": "^2.4.1",
    "vite": "^2.8.0"
  },
  "lint-staged": {
    "*.{js,ts,jsx,vue}": [
      "eslint --fix --color",
      "git add"
    ],
    "*.{css,less,scss}": [
      "prettier --fix",
      "git add"
    ]
  }
}
